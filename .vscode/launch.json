    "version": "0.2.0",
    "configurations": [
        {
            "name": "cli-client",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-client",
            "program": "${workspaceFolder}/client/bin/Debug/net5.0/client.dll",
            "args": [
            ],
            "cwd": "${workspaceFolder}/client",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "deploy",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/sc/NeoContributorToken.nef",
            "neo-express": "${workspaceFolder}/default.neo-express",
            "invocation": "deploy",
            "deploy-signer": "@owen"
        },
        {
            "name": "mint (succeed)",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/sc/NeoContributorToken.nef",
            "neo-express": "${workspaceFolder}/default.neo-express",
            "checkpoint": "${workspaceFolder}/checkpoints/contract-deployed.neoxp-checkpoint",
            "invocation": {
                "operation": "mint",
                "args": [
                    "Da Hongfei",
                    "Founder of Neo, Chair of Neo Foundation, CEO of NGD",
                    "https://neo3.azureedge.net/images/discover/DaHongfei.jpg"
                ]
            },
            "signers": [
                "@owen"
            ]
        },
        {
            "name": "mint (fail)",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/sc/NeoContributorToken.nef",
            "neo-express": "${workspaceFolder}/default.neo-express",
            "checkpoint": "${workspaceFolder}/checkpoints/contract-deployed.neoxp-checkpoint",
            "invocation": {
                "operation": "mint",
                "args": [
                    "Da Hongfei",
                    "Founder of Neo, Chair of Neo Foundation, CEO of NGD",
                    "https://neo3.azureedge.net/images/discover/DaHongfei.jpg"
                ]
            },
            "signers": [
                "@alice"
            ]
        },
        {
            "name": "mint second (succeed)",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/sc/NeoContributorToken.nef",
            "neo-express": "${workspaceFolder}/default.neo-express",
            "checkpoint": "${workspaceFolder}/checkpoints/hongfei-token-minted.neoxp-checkpoint",
            "invocation": {
                "operation": "mint",
                "args": [
                    "Erik Zhang",
                    "Founder and Core developer of Neo, Chair of Neo Foundation",
                    "https://neo3.azureedge.net/images/discover/ErikZhang.jpg"
                ]
            },
            "signers": [
                "@owen"
            ]
        },
        {
            "name": "buy-hongfei-nft",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/sc/NeoContributorToken.nef",
            "neo-express": "${workspaceFolder}/default.neo-express",
            "checkpoint": "${workspaceFolder}/checkpoints/hongfei-token-minted.neoxp-checkpoint",
            "invocation": {
                "invoke-file": "${workspaceFolder}/invoke-files/buy-hongfei-nft.json"
            },
            "signers": [
                "@alice"
            ]
        },

    ]
}